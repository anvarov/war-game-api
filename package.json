{
  "name": "war-game-rest",
  "packageManager": "yarn@3.1.1",
  "private": true,
  "devDependencies": {
    "@types/eslint": "^8",
    "@types/express": "^4",
    "@types/prettier": "^2",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": ">=6",
    "jest": "^27.4.7",
    "lint-staged": ">=10",
    "pinst": "^2.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "express": "^4.17.2",
    "helmet": "^5.0.1"
  },
  "scripts": {
